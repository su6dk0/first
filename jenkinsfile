pipeline {
    agent any // This means the pipeline can run on any available agent (node)

    stages {
        stage('Checkout') {
            steps {
                // Checkout the code from your version control system (e.g., Git)
                script {
                    git 'https://github.com/su6dk0/first.git'
                }
            }
        }

        stage('Build') {
            steps {
                // Build your application (e.g., compile code)
                script {
                    sh 'mvn clean install' // Example for Maven build tool
                }
            }
        }

        stage('Deploy') {
            steps {
                // Deploy your application (e.g., to a server or a container)
                script {
                   echo "deploy"
                }
            }
        }
    }
}

